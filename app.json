{
  "expo": {
    "name": "runic",
    "slug": "runic",
    "scheme": "runic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#000000",
      "image": "./src/assets/icon.png",
      "resizeMode": "native"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.domeicu.runic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.domeicu.runic"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-notifications",
      "@react-native-community/datetimepicker",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./src/assets/icon.png",
          "resizeMode": "native"
        }
      ]
    ]
  }
}
